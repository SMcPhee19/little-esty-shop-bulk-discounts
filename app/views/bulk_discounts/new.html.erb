<h1>Bulk Discount: New</h1>
<br>
<div id="new-merchant-bulk">
  <%= form_with url: "/merchants/#{@merchant.id}/bulk_discounts/new", method: :post do |form|  %>
    <%= form.label :name %>
    <%= form.text_field :name %>

    <%= form.label :percent %>
    <%= form.number_field :percent %>
    
    <%= form.label :quantity %>
    <%= form.number_field :quantity %>
    <%= form.submit %>
  <% end %>
</div>